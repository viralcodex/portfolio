{
  "name": "portfolio-curl",
  "version": "1.0.0",
  "description": "A curl-able portfolio page",
  "keywords": [
    "ascii",
    "curl",
    "portfolio"
  ],
  "license": "MIT",
  "author": "Your Name",
  "main": "server.js",
  "scripts": {
    "start": "bun run server.js",
    "dev": "bun --watch server.js",
    "build:css": "tailwindcss -i public/styles.css -o public/output.css",
    "watch:css": "tailwindcss -i public/styles.css -o public/output.css --watch",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "format": "oxfmt .",
    "format:check": "oxfmt --check"
  },
  "dependencies": {
    "express": "^4.18.2",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "pdf-parse": "^2.4.5"
  }
}
