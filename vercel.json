{
  "version": 2,
  "builds": [
    {
      "src": "server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.+\\.(html|css|js|ico|png|jpg|jpeg|svg|gif))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.ts"
    }
  ]
}
